digraph HealthcareClinicERD {
	rankdir=LR
	A [label=Patients]
	B [label=Appointments]
	C [label=MedicalStaff]
	D [label=PrimaryCarePhysicians]
	E [label=Medications]
	F [label=Prescriptions]
	G [label=LabTests]
	H [label=LabResults]
	I [label=Bills]
	J [label=Payments]
	A -> D [label="Has Primary Care Physician" arrowhead=crow headlabel=M taillabel=1]
	C -> D [label="Is Primary Care Physician" arrowhead=crow headlabel=M taillabel=1]
	A -> B [label="Has Appointment" arrowhead=crow headlabel=M taillabel=1]
	C -> B [label="Schedules Appointment" arrowhead=crow headlabel=M taillabel=1]
	A -> F [label="Receives Prescription" arrowhead=crow headlabel=M taillabel=1]
	E -> F [label="Is Prescribed" arrowhead=crow headlabel=M taillabel=1]
	A -> H [label="Has Lab Result" arrowhead=crow headlabel=M taillabel=1]
	G -> H [label="Is Tested" arrowhead=crow headlabel=M taillabel=1]
	A -> J [label="Makes Payment" arrowhead=crow headlabel=M taillabel=1]
	I -> J [label="Is Paid" arrowhead=crow headlabel=M taillabel=1]
}
